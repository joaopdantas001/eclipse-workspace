package moedaconversor;

public class TesteConversao extends Moeda{
	
	public static void main(String[] args) {
		
		ConversorMoeda brl = new ConversorMoeda();
		
		brl.setData("07/12/2022");
		brl.setValorparaconversao(10);
		brl.setPais("Brasil");
		brl.setNomemoeda("Real");
		brl.setSigla("R$");
		brl.setCodigo("BRL");
		brl.setCambiobrl(1.0);
		brl.setCambiousd(0.1911);
		brl.setCambioeur(0.1816);
		brl.setCambiogbp(0.1569);
		brl.setCambiojpy(26.2075);
		
		
		ConversorMoeda usd = new ConversorMoeda();
		
		usd.setData("07/12/2022");
		usd.setValorparaconversao(20);
		usd.setPais("Estados Unidos");
		usd.setNomemoeda("Dolar Americano");
		usd.setSigla("US$");
		usd.setCodigo("USD");
		usd.setCambiobrl(1.0);
		usd.setCambiousd(0.1911);
		usd.setCambioeur(0.1816);
		usd.setCambiogbp(0.1569);
		usd.setCambiojpy(26.2075);
		
		ConversorMoeda eur = new ConversorMoeda();
		
		usd.setData("07/12/2022");
		usd.setValorparaconversao(30);
		usd.setPais("Uni√£o Europeia");
		usd.setNomemoeda("Euro");
		usd.setSigla("$");
		usd.setCodigo("EUR");
		usd.setCambiobrl(1.0);
		usd.setCambiousd(0.1911);
		usd.setCambioeur(0.1816);
		usd.setCambiogbp(0.1569);
		usd.setCambiojpy(26.2075);
		
		System.out.println(brl.getData());
		System.out.println("     " + " " + brl.getCodigo() + " " + usd.getCodigo() + " ");
		System.out.println(" " + brl.getCodigo() + " ");
		System.out.println(" " + usd.getCodigo() + " ");
		
