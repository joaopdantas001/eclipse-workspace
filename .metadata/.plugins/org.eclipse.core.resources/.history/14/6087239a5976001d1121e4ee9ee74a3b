
public class Moeda {
	
	private String pais;
	private String descricaomoeda;
	private int codigosiscomex;
	private String simbolomonetario;
	private String simboloiso;
	private double cambiobrl;
	private double cambiousd;
	private double cambioeur;
	private double cambiogbp;
	private double cambiojpy;
	
	protected String getPais() {
		return pais;
	}
	protected void setPais(String pais) {
		this.pais = pais;
	}
	protected String getDescricaomoeda() {
		return descricaomoeda;
	}
	protected void setDescricaomoeda(String descricaomoeda) {
		this.descricaomoeda = descricaomoeda;
	}
	protected int getCodigosiscomex() {
		return codigosiscomex;
	}
	protected void setCodigosiscomex(int codigosiscomex) {
		this.codigosiscomex = codigosiscomex;
	}
	protected String getSimbolomonetario() {
		return simbolomonetario;
	}
	protected void setSimbolomonetario(String simbolomonetario) {
		this.simbolomonetario = simbolomonetario;
	}
	protected String getSimboloiso() {
		return simboloiso;
	}
	protected void setSimboloiso(String simboloiso) {
		this.simboloiso = simboloiso;
	}
	protected double getCambiobrl() {
		double roundOff = Math.round(cambiobrl*100)/100.00;
		return roundOff;
	}
	protected void setCambiobrl(double cambiobrl) {
		this.cambiobrl = cambiobrl;
	}
	protected double getCambiousd() {
		return cambiousd;
	}
	protected void setCambiousd(double cambiousd) {
		this.cambiousd = cambiousd;
	}
	protected double getCambioeur() {
		return cambioeur;
	}
	protected void setCambioeur(double cambioeur) {
		this.cambioeur = cambioeur;
	}
	protected double getCambiogbp() {
		return cambiogbp;
	}
	protected void setCambiogbp(double cambiogbp) {
		this.cambiogbp = cambiogbp;
	}
	protected double getCambiojpy() {
		return cambiojpy;
	}
	protected void setCambiojpy(double cambiojpy) {
		this.cambiojpy = cambiojpy;
	}
	
}
